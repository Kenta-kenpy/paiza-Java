        String s_1 = sc.next();
        String s_2 = sc.next();
        String s_3 = sc.next();
        String s_4 = sc.next();
        String s_5 = sc.next();

        System.out.println(s_1 + " " + s_2);
        System.out.println(s_3 + " " + s_4 + " " + s_5);
    }
}

// 和室
import java.util.*;

public class Main {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String s_1 = sc.next();
        String s_2 = sc.next();
        String s_3 = sc.next();
        String s_4 = sc.next();
        String s_5 = sc.next();
        String s_6 = sc.next();

        System.out.println(s_3 + " " + s_1 + " " + s_2);
        System.out.println(s_6 + " " + s_4 + " " + s_5);
    }
}

// 円卓
import java.util.*;

public class Main {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String s_1 = sc.next();
        String s_2 = sc.next();
        String s_3 = sc.next();
        String s_4 = sc.next();
        String s_5 = sc.next();
        String s_6 = sc.next();
        String s_7 = sc.next();
        String s_8 = sc.next();

        System.out.println(s_3 + " " + s_1 + " " + s_2);
        System.out.println(s_5 + " _ " + s_4);
        System.out.println(s_7 + " " + s_8 + " " + s_6);
    }
}
