import java.util.*;

public class Main {
    public static void main(String[] args) {
        // æ¨™æº–å…¥åŠ›ã‚’æ‰±ã†ãŸã‚ã®Scannerã‚ªãƒ–ã‚¸ã‚§ã‚¯ãƒˆã‚’ä½œæˆ
        Scanner sc = new Scanner(System.in);

        // 1è¡Œç›®ã®å…¥åŠ›ã‚’æ•´æ•°ã¨ã—ã¦èª­ã¿å–ã‚Šã€å¤‰æ•°aã«æ ¼ç´
        int a = Integer.parseInt(sc.nextLine());
        
        // 2è¡Œç›®ã®å…¥åŠ›ã‚’æ–‡å­—åˆ—ã¨ã—ã¦èª­ã¿å–ã‚Šã€å¤‰æ•°bã«æ ¼ç´
        String b = sc.nextLine();
        
        // 3è¡Œç›®ã®å…¥åŠ›ã‚’æ–‡å­—åˆ—ã¨ã—ã¦èª­ã¿å–ã‚Šã€å¤‰æ•°cã«æ ¼ç´
        String c = sc.nextLine();
        
        // 4è¡Œç›®ã®å…¥åŠ›ã‚’æ–‡å­—åˆ—ã¨ã—ã¦èª­ã¿å–ã‚Šã€å¤‰æ•°dã«æ ¼ç´
        String d = sc.nextLine();
        
        // 5è¡Œç›®ã®å…¥åŠ›ã‚’æ–‡å­—åˆ—ã¨ã—ã¦èª­ã¿å–ã‚Šã€å¤‰æ•°eã«æ ¼ç´
        String e = sc.nextLine();

        // æ¡ä»¶: aãŒ0ã‚ˆã‚Šå¤§ãã„ãªã‚‰"plus"ã‚’å‡ºåŠ›ã€ãã‚Œä»¥å¤–ã®å ´åˆã¯ãã®ã¾ã¾aã‚’å‡ºåŠ›
        System.out.println(a > 0 ? "plus" : a);

        // æ¡ä»¶: bãŒ"hoge"ã¨ä¸€è‡´ã™ã‚‹ãªã‚‰"yes"ã‚’å‡ºåŠ›ã€ãã‚Œä»¥å¤–ã®å ´åˆã¯ãã®ã¾ã¾bã‚’å‡ºåŠ›
        System.out.println(b.equals("hoge") ? "yes" : b);

        // æ¡ä»¶: cã®æ–‡å­—åˆ—é•·ãŒ10ãªã‚‰"ten"ã‚’å‡ºåŠ›ã€ãã‚Œä»¥å¤–ã®å ´åˆã¯ãã®ã¾ã¾cã‚’å‡ºåŠ›
        System.out.println(c.length() == 10 ? "ten" : c);

        // æ¡ä»¶: dã«"x"ãŒå«ã¾ã‚Œã¦ã„ã‚‹å ´åˆã€æœ€åˆã«è¦‹ã¤ã‹ã£ãŸä½ç½®(0å§‹ã¾ã‚Š)ã‚’å‡ºåŠ›ã€
        //       å«ã¾ã‚Œã¦ã„ãªã„å ´åˆã¯"nothing"ã‚’å‡ºåŠ›
        System.out.println(d.contains("x") ? d.indexOf("x") : "nothing");

        // æ¡ä»¶: eã®æ–‡å­—åˆ—é•·ãŒ5ãªã‚‰"five"ã‚’å‡ºåŠ›ã€
        //       ãã‚Œä»¥å¤–ã®å ´åˆã¯eã®æœ€åˆã®1æ–‡å­—ã‚’å‡ºåŠ›
        System.out.println(e.length() == 5 ? "five" : e.charAt(0));
    }
}
